{
  "name": "tomchochola/php-cs-fixer",
  "description": "PHP CS Fixer",
  "license": "proprietary",
  "type": "library",
  "keywords": [
    "tomchochola",
    "php-cs-fixer"
  ],
  "authors": [
    {
      "name": "Tomáš Chochola",
      "email": "chocholatom1997@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "friendsofphp/php-cs-fixer": "^3.8"
  },
  "require-dev": {
    "composer/composer": "^2.0",
    "phpunit/phpunit": "^9.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Tomchochola\\PhpCsFixer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tomchochola\\PhpCsFixer\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
