{
  "require-dev": {
    "tomchochola/phpstan-rules": "dev-main"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/tomchochola/phpstan-rules"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "sort-packages": true
  }
}
